#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    int x=0;
    int y=0;
    cin>>t;
    while(t--)
    {
        cin>>x>>y;
        if(x==y)
        {
            if(x==0 && y==0)
            {
                cout<<"NO"<<endl;
            }
            else
            {
                cout<<"YES"<<endl;
            }
        }
       
        else
        {
            cout<<"NO"<<endl;
        }
    }
	// your code goes here
	return 0;
}
