#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    int k=0;
    cin>>t;
    while(t--)
    {
        cin>>k;
        if(k%2==0)
        {
            cout<<k<<endl;
        }
        else
        {
            cout<<k+2<<endl;
        }
    }
	// your code goes here
	return 0;
}
