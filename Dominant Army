#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    int Na=0;
    int Nb=0;
    int Nc=0;
    cin>>t;
    while(t--)
    {
        cin>>Na>>Nb>>Nc;
       if(Na>Nb+Nc || Nb>Na+Nc || Nc>Na+Nb)
       {
           cout<<"YES"<<endl;
       }
       else
       {
           cout<<"NO"<<endl;
       }
    }
	// your code goes here
	return 0;
}
