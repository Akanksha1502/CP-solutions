#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    int n=0;
    int counter=0;
    string b;
    cin>>t;
    while(t--)
    {
        cin>>n;
        cin>>b;
        counter=0;
        
        
        for(int i=0; i<n; i++)
        {
            if(b[i]=='0')
            {
                counter++;
            }
        }
        if((((120-counter)*100)/120)>=75)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
	// your code goes here
	return 0;
}
