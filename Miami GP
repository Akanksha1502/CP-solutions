#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    float x=0;
    float y=0;
    cin>>t;
    while(t--)
    {
        cin>>x>>y;
        if(y<=x*1.07)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
	// your code goes here
	return 0;
}
