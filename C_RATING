#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;;
    int X=0;  
    int Y=0;
    cin>>t;
    while(t--)
    {
        cin>>X>>Y;
        int s=0;
        s=Y-X;
        if(s%8==0)
        {
            cout<<s/8<<endl;
        }
        else
        {
            cout<<s/8+1<<endl;
        }
        
    }
	// your code goes here
	return 0;
}
