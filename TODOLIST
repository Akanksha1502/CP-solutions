#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    cin>>t;
    while(t--)
    {
        int n=0;
        int counter=0;
        cin>>n;
        for(int i=0; i<n; i++)
        {
            int inp=0;
            cin>>inp;
            if(inp>=1000)
            {
                counter++;
            }
        }
        cout<<counter<<endl;
    }
	// your code goes here
	return 0;
}
