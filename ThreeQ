#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    cin>>t;
    while(t--)
    {
    int A1,A2,A3;
    int B1,B2,B3;
        cin>>A1>>A2>>A3;
        cin>>B1>>B2>>B3;
        if((A1+A2+A3)==(B1+B2+B3))
        {
            cout<<"Pass"<<endl;
        }
        else
        {
            cout<<"Fail"<<endl;
        }
    }
	// your code goes here
	return 0;
}
