#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    int t=0;
    int x=0;
    int k=0;
    int n=0;
    cin>>t;
    while(t--)
    {
        cin>>n>>x>>k;
        if((n*x)<=k)
        {
            cout<<"YES"<<endl;
            
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
	// your code goes here
	return 0;
}
